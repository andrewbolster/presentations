    "slidev-theme",
    "slidev",
    "black-duck"
  ],
  "author": "Andrew Bolster",
  "license": "MIT",
  "peerDependencies": {
    "@vueuse/core": ">=10.0.0"
  },
  "slidev": {
    "colorSchema": "both",
    "highlighter": "shiki",
    "defaults": {
      "fonts": {
        "sans": "Roboto",
        "mono": "Roboto Mono"
      },
      "themeConfig": {
        "primary": "#5a2a82",
        "secondary": "#0072a3"
      }
    }
  }
}
